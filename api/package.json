{
    "name": "@recursyve/seed",
    "version": "1.0.0",
    "description": "",
    "author": "",
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "build:data-migrations": "rimraf data-migrations-dist && tsc --build tsconfig.migrations.json",
        "format": "prettier --write src/**/*.ts test/**/*.ts",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint . --fix --ext .ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "install": "(ls foodapp/pre-commit-setup.sh >> /dev/null 2>&1 && echo 'pre-commit setup' && chmod +x ../pre-commit-setup.sh && bash -c ../pre-commit-setup.sh) || echo 'pre-commit setup skipped'",
        "data-migrations": "node data-migrations-dist/data-migrations/index.js"
    },
    "dependencies": {
        "@nestjs/axios": "^3.1.3",
        "@nestjs/common": "^10.4.15",
        "@nestjs/core": "^10.4.15",
        "@nestjs/platform-express": "^10.4.15",
        "@nestjs/sequelize": "^10.0.1",
        "@nestjs/terminus": "^10.2.3",
        "@recursyve/nestjs-common": "^10.0.0",
        "@recursyve/nestjs-config": "^10.0.0",
        "@recursyve/nestjs-data-filter": "^9.4.0",
        "@recursyve/nestjs-rosetta-core": "^10.0.0-beta.26",
        "@recursyve/nestjs-rosetta-sequelize": "^10.0.0-beta.26",
        "@recursyve/nestjs-sequelize-utils": "^10.1.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "compression": "^1.7.5",
        "dotenv": "^16.0.1",
        "helmet": "^5.1.0",
        "morgan": "^1.10.0",
        "pg": "^8.13.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.5.5",
        "sequelize": "^6.21.2",
        "sequelize-typescript": "^2.1.3"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.4.9",
        "@nestjs/schematics": "^8.0.11",
        "@nestjs/testing": "^10.4.15",
        "@recursyve/eslint-config-node": "^1.0.0",
        "@types/compression": "^1.7.2",
        "@types/express": "^4.17.13",
        "@types/jest": "^28.1.3",
        "@types/morgan": "^1.9.3",
        "@types/node": "^16.11.41",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.30.0",
        "@typescript-eslint/parser": "^5.30.0",
        "eslint": "^8.18.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.1.0",
        "jest": "^28.1.1",
        "prettier": "^2.7.1",
        "supertest": "^6.2.3",
        "ts-jest": "^28.0.5",
        "ts-loader": "^9.3.1",
        "ts-node": "^10.8.1",
        "tsconfig-paths": "^4.0.0",
        "typescript": "^4.7.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "./coverage",
        "testEnvironment": "node"
    }
}
